{
  "name": "note-manager",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "yarn install && next dev -p 8000",
    "build": "next build",
    "start": "next build && cd src && next dev",
    "lint": "yarn eslint .",
    "server": "cd server && yarn install && yarn start",
    "serve": "concurrently --kill-others \"yarn server\" \"yarn dev\""
  },
  "dependencies": {
    "@heroicons/react": "^2.0.18",
    "axios": "^1.4.0",
    "next": "13.4.5",
    "notistack": "^3.0.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-select": "^5.7.4",
    "swr": "^2.2.0",
    "tailwindcss": "^3.3.2"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "@typescript-eslint/parser": "^5.59.11",
    "encoding": "^0.1.13",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "prettier": "^2.8.8",
    "prettier-plugin-tailwindcss": "^0.3.0",
    "concurrently": "^8.2.0",
    "eslint": "^8.42.0",
    "eslint-config-next": "13.4.5",
    "autoprefixer": "10.4.14",
    "@types/node": "20.3.0",
    "@types/react": "18.2.11",
    "@types/react-dom": "18.2.4",
    "typescript": "^5.1.3",
    "postcss": "8.4.24"
  }
}
